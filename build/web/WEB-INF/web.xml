        <servlet-name>ManageAdmin</servlet-name>
        <url-pattern>/manageAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertAdminController</servlet-name>
        <url-pattern>/insertAdmin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
